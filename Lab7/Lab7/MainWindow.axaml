<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="Lab7.MainWindow"
        Width="500" Height="500"
        Title="DNA">

	<StackPanel Margin="15" Spacing="5">
		<TextBlock Text="Sekwencja DNA:"/>
		<TextBox Name="DNA" AcceptsReturn="True" Height="100"/>

		<Button Content="Oblicz" Click="DNAClick"/>

		<TextBlock Text="Wyniki:"/>
		<ScrollViewer Height="280">
			<ListBox Name="ResultsBox"/>
		</ScrollViewer>
	</StackPanel>
</Window>